{
  "name": "@pentamania/phina-extensions",
  "version": "0.4.0",
  "description": "phina.js extensions for various game projects",
  "main": "dist/phina-extensions.js",
  "main_min": "dist/phina-extensions.min.js",
  "module": "dist/phina-extensions.esm.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "node prebuild && rollup -w -c",
    "build": "node prebuild && rollup -c",
    "pack": "npm run build && npm pack",
    "doc": "npm run doc:clean && npm run doc:build",
    "doc:clean": "del-cli docs/",
    "doc:build": "jsdoc -c jsdoc.conf.json -R README.md",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "phina.js"
  ],
  "author": "pentamania",
  "license": "MIT",
  "devDependencies": {
    "@pixi/jsdoc-template": "github:pentamania/pixi-jsdoc-template#phina-custom",
    "del-cli": "^3.0.0",
    "jsdoc": "^3.6.3",
    "rollup": "^1.19.4",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "dependencies": {}
}
